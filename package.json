{
    "name": "q-require",
    "description": "asynchronous CommonJS require module loader for Node and browsers",
    "version": "0.0.3",
    "implements": [
        "CommonJS/Modules/1.1.1",
        "CommonJS/Modules/Async/A"
    ],
    "homepage": "http://github.com/kriskowal/q-require/",
    "author": "Kris Kowal <kris@cixar.com> (http://github.com/kriskowal/)",
